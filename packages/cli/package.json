{
  "name": "@vcd/ext-cli",
  "description": "CLI tool for working with Cloud Director extensions",
  "version": "0.0.0",
  "author": "mslavov@vmware.com",
  "bin": {
    "vcd-ext": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@vcd/ext-compiler": "~0.0.0",
    "@vcd/node-client": "~0.0.0",
    "glob": "^7.1.3",
    "js-yaml": "^3.13.1",
    "rimraf": "^3.0.0",
    "tslib": "^1.10.0",
    "yeoman-environment": "^2.4.0",
    "yeoman-generator": "^4.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "8.10.34",
    "@types/yeoman-environment": "^2.3.2",
    "@types/yeoman-generator": "^3.0.2",
    "@types/yosay": "^0.0.29",
    "@types/glob": "^7.1.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/templates",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vcd-ext",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "build": "tsc",
    "setup-dev": "npm link"
  },
  "types": "lib/index.d.ts"
}
