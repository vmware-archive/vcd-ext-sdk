{
  "name": "@vcd/ext-cli",
  "description": "CLI tool for working with Cloud Director extensions",
  "version": "0.0.12-alpha.14",
  "author": "VMware",
  "license": "BSD-2",
  "bin": {
    "vcd-ext": "bin/run"
  },
  "dependencies": {
    "@cloudflare/json-schema-walker": "^0.1.1",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@vcd/care-package": "0.0.1-alpha.8",
    "@vcd/node-client": "0.0.6-alpha.2",
    "adm-zip": "^0.4.16",
    "debug": "^4.2.0",
    "glob": "^7.1.3",
    "is-my-json-valid": "^2.20.5",
    "js-yaml": "^3.13.1",
    "rimraf": "^3.0.2",
    "tslib": "^1.10.0",
    "uuid": "^8.3.1",
    "yeoman-environment": "^2.4.0",
    "yeoman-generator": "^4.12.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/adm-zip": "^0.4.33",
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.3",
    "@types/jasmine": "^3.5.4",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "10.12.0",
    "@types/rimraf": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-environment": "^2.3.2",
    "@types/yeoman-test": "^2.0.5",
    "@types/yosay": "^0.0.29",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.3.0",
    "nock": "^13.0.4",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "tslint": "5.15.0",
    "typescript": "~4.0.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^3.0.0"
  },
  "engines": {
    "node": ">=10.14.2 <13.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/templates",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init/analytics",
      "prerun": "./lib/hooks/prerun/analytics",
      "postrun": "./lib/hooks/postrun/analytics"
    },
    "bin": "vcd-ext",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "version": "oclif-dev readme && git add README.md",
    "build": "tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jasmine-ts --config=jasmine.json",
    "test-ci": "npm run test",
    "coverage": "nyc -r text -e .ts -x \"**/*.[sS]pec.ts\" npm run test"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
