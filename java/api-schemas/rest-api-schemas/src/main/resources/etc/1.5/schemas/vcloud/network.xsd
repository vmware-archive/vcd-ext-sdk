     <xs:complexType name="FirewallRuleType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a firewall rule.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="Id" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Firewall rule identifier.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="IsEnabled" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Used to enable or disable the firewall rule. Default value is true.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="MatchOnTranslate" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.1" removed-in="REMOVED_VERSION_15_51"/></xs:appinfo>
                            <xs:documentation source="since">5.1</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                For DNATed traffic, match the firewall rules only after
                                the destination IP is translated.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Description" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                A description of the rule.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Policy" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                One of:
                                drop (drop packets that match the rule),
                                allow (allow packets that match the rule to pass
                                through the firewall)
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Protocols" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Specify the protocols to which the rule should be applied.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:choice>
                                <xs:sequence>
                                    <xs:element maxOccurs="1" minOccurs="0" name="Tcp" type="xs:boolean">
                                        <xs:annotation>
                                            <xs:documentation source="modifiable">always</xs:documentation>
                                            <xs:documentation xml:lang="en">
                                                True if the rule applies to the
                                                TCP protocol.
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element maxOccurs="1" minOccurs="0" name="Udp" type="xs:boolean">
                                        <xs:annotation>
                                            <xs:documentation source="modifiable">always</xs:documentation>
                                            <xs:documentation xml:lang="en">
                                                True if the rule applies to the
                                                UDP protocol.
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                                <xs:element maxOccurs="1" minOccurs="0" name="Icmp" type="xs:boolean">
                                     <xs:annotation>
                                         <xs:documentation source="modifiable">always</xs:documentation>
                                         <xs:documentation xml:lang="en">
                                                True if the rule applies to the
                                                ICMP protocol.
                                         </xs:documentation>
                                     </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="1" minOccurs="0" name="Any" type="xs:boolean">
                                     <xs:annotation>
                                         <xs:documentation source="modifiable">always</xs:documentation>
                                         <xs:documentation xml:lang="en">
                                                True if the rule applies to any
                                                protocol.
                                         </xs:documentation>
                                     </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="1" minOccurs="0" name="Other" type="xs:string">
                                     <xs:annotation>
                                         <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                                         <xs:documentation source="modifiable">always</xs:documentation>
                                         <xs:documentation xml:lang="en">
                                                Any other protocol supported by vShield Manager
                                         </xs:documentation>
                                     </xs:annotation>
                                </xs:element>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="IcmpSubType" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                ICMP subtype. One of: address-mask-request, address-mask-reply, destination-unreachable,
                                echo-request, echo-reply, parameter-problem, redirect, router-advertisement,
                                router-solicitation, source-quench, time-exceeded, timestamp-request, timestamp-reply, any.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Port" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version removed-in="REMOVED_VERSION_15_51"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The port to which this rule applies. A value of
                                -1 matches any port.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="DestinationPortRange" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                            <xs:documentation source="since">5.1</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Destination port range to which this rule applies.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:choice>
                        <xs:element name="DestinationIp" type="FirewallIpAddressType" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation source="since">1.0</xs:documentation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    Destination IP address to which the rule
                                    applies. A value of Any matches any IP address.
                                </xs:documentation>
                                <xs:documentation source="required">false</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="DestinationVm" type="VmSelectionType" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                                <xs:documentation source="since">5.1</xs:documentation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    Details of the destination VM
                                </xs:documentation>
                                <xs:documentation source="required">false</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                    <xs:element name="SourcePort" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.5</xs:documentation>
                            <xs:appinfo><meta:version removed-in="REMOVED_VERSION_15_51"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Destination port to which this rule applies.
                                A value of -1 matches any port.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SourcePortRange" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                            <xs:documentation source="since">5.1</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Source port range to which this rule applies.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:choice>
                        <xs:element name="SourceIp" type="FirewallIpAddressType" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation source="since">1.5</xs:documentation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    Source IP address to which the rule applies.
                                    A value of Any matches any IP address.
                                </xs:documentation>
                                <xs:documentation source="required">false</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="SourceVm" type="VmSelectionType" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                                <xs:documentation source="since">5.1</xs:documentation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    Details of the source Vm
                                </xs:documentation>
                                <xs:documentation source="required">false</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                    <xs:element name="Direction" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.5</xs:documentation>
                            <xs:appinfo><meta:version removed-in="REMOVED_VERSION_15_51"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
