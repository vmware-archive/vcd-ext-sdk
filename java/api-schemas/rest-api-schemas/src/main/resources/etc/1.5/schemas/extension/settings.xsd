                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ValidSerial" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Flag indicating whether the provided serial is valid. This is a read only
                                field for the API client. The license manager will set this flag appropriately.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
