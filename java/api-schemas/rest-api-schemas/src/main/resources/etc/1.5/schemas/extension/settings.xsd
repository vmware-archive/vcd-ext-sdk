                        </xs:annotation>
                    </xs:element>
                    <xs:element name="AlertEmailTo" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                A comma-separated list of email addresses to
                                send alert messages to.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SmtpSettings" type="SmtpSettingsType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">
                                The SMTP server for email notifications and alerts.
                            </xs:documentation>
                            <xs:documentation source="required">true</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SmtpSettingsType">
        <xs:annotation>
             <xs:documentation source="since">1.5</xs:documentation>
             <xs:documentation xml:lang="en">
                 The SMTP server for email notifications and alerts.
             </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ResourceType">
                <xs:sequence>
                    <xs:element name="UseAuthentication" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">
                                A flag to indicate if the SMTP requires login authentication. If this
                                flag is set to true, caller must provide user name and password.
                            </xs:documentation>
                            <xs:documentation source="required">true</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SmtpServerName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">
                                SMTP server hostname.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SmtpServerPort" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">
                                SMTP server port.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ssl" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">
                                True if the SMTP server requires an SSL
                                connection.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                            <xs:documentation source="deprecated">11.0</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SmtpSecureMode" type="vcloud:SmtpSecureModeType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="11.0" /></xs:appinfo>
                            <xs:documentation source="since">11.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Security protocol to use when connecting to the SMTP server. One of:&lt;br&gt;
                                startTls -- Use the STARTTLS protocol&lt;br&gt;
                                ssl -- -Use the SSL/TLS protocol&lt;br&gt;
                                none -- Do not use any security protocol
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SslTrustStore" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="11.0" /></xs:appinfo>
                            <xs:documentation source="since">11.0</xs:documentation>
                            <xs:documentation xml:lang="en">
                                 Custom truststore to use to trust peer certificates when establishing a secure connection using SSL/STARTTLS (when enabled).
                            </xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="UserName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">
                                SMTP admin user name. 128 characters or less.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Password" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">
                                SMTP admin password. 128 characters or less.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="KerberosSettings" type="KerberosSettingsType"/>
    <xs:complexType name="KerberosSettingsType">
        <xs:annotation>
            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
            <xs:appinfo>
                <meta:content-type>application/vnd.vmware.admin.kerberosSettings</meta:content-type>
            </xs:appinfo>
            <xs:documentation source="since">5.1</xs:documentation>
            <xs:documentation xml:lang="en">
               Global Kerberos settings.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ResourceType">
                <xs:sequence>
                    <xs:element name="Realm" type="RealmType" minOccurs="0" maxOccurs="unbounded" >
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                            <xs:documentation source="required">true</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="allowLowerCaseRealms" type="xs:boolean">
                    <xs:annotation>
                        <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                        <xs:documentation xml:lang="en">
                           A value of true allows the use of lower-case characters in realm names.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RealmType">
        <xs:annotation>
            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
            <xs:documentation source="since">5.1</xs:documentation>
            <xs:documentation xml:lang="en">
                Kerberos realm information.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="Domain" minOccurs="0" maxOccurs="unbounded" type="xs:string">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                            <xs:documentation>
                                The list of all DNS suffixes associated with the realm.
                            </xs:documentation>
                            <xs:documentation source="required">true</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="kdc" type="xs:string">
                    <xs:annotation>
                        <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                        <xs:documentation xml:lang="en">
                            The KDC for the current realm.
                        </xs:documentation>
                        <xs:documentation source="required">true</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="name" type="xs:string">
                    <xs:annotation>
                        <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                        <xs:documentation xml:lang="en">
                            The name of the realm. Must be all upper-case characters unless AllowLowerCaseRealms is true.
                        </xs:documentation>
                        <xs:documentation source="required">true</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="LookupServiceSettings" type="LookupServiceSettingsType" />
    <xs:element name="LookupServiceParams" type="LookupServiceParamsType" />

    <xs:complexType name="LookupServiceSettingsType">
        <xs:annotation>
            <xs:appinfo>
                <meta:content-type>application/vnd.vmware.admin.lookupServiceSettings</meta:content-type>
                <meta:version added-in="5.1"/>
            </xs:appinfo>
            <xs:documentation source="since">5.1</xs:documentation>
            <xs:documentation xml:lang="en">
                Information that allows vCloud Director to connect to a
                vSphere lookup service.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="vcloud:ResourceType">
                <xs:sequence>
                    <xs:element name="LookupServiceUrl" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                            <xs:documentation>
                                The URL of the vSphere Lookup Service. If empty or missing on PUT, any registered lookup service is unregistered.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LookupServiceParamsType">
        <xs:annotation>
            <xs:appinfo>
                <meta:content-type>application/vnd.vmware.admin.lookupServiceParams</meta:content-type>
                <meta:version added-in="5.1"/>
            </xs:appinfo>
            <xs:documentation source="since">5.1</xs:documentation>
            <xs:documentation xml:lang="en">
                Parameters for a register or unregister Lookup Service request.
                If LookupServiceUrl is present and not empty, the request
                registers a lookup service. If LookupServiceUrl is missing
                or empty, the request unregisters a lookup service.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="LookupServiceSettingsType">
                <xs:sequence>
                    <xs:element name="VCDUrl" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation source="deprecated">5.6</xs:documentation>
                            <xs:documentation xml:lang="en">
                               The vCloud Director public login URL. This
                               element is deprecated and should be left empty
                               when using API version 5.6 and later, which
                               obtain this address from
                               SystemSettings:GeneralSettings:SystemExternalAddress.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="userName" type="xs:string" use="required">
                    <xs:annotation>
                        <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            The vSphere SSO service user name of a user who has administrative privileges.
                        </xs:documentation>
                        <xs:documentation source="required">true</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="password" type="xs:string" use="required">
                    <xs:annotation>
                        <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            The vSphere SSO service password for the user named in userName.
                        </xs:documentation>
                        <xs:documentation source="required">true</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="VcTrustStoreUpdateParams" type="VcTrustStoreUpdateParamsType"/>
    <xs:complexType name="VcTrustStoreUpdateParamsType">
        <xs:annotation>
            <xs:appinfo>
                <meta:version added-in="5.1" />
                <meta:content-type>application/vnd.vmware.admin.vcTrustStoreUpdateParams</meta:content-type>
            </xs:appinfo>
            <xs:documentation source="since">5.1</xs:documentation>
            <xs:documentation xml:lang="en">
                Parameters for updating the vCenter truststore.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:FileUploadParamsType">
                <xs:attribute name="type" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                        <xs:documentation source="modifiable">create</xs:documentation>
                        <xs:documentation xml:lang="en">
                           Type of the trust store. One of:&lt;br&gt;
                           JKS&lt;br&gt;
                           JCEKS&lt;br&gt;
                           PKCS12
                        </xs:documentation>
                        <xs:documentation source="required">true</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="password" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                        <xs:documentation source="modifiable">create</xs:documentation>
                        <xs:documentation xml:lang="en">
                           Password for the vCenter trust store.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="VcTrustStoreUploadSocket" type="VcTrustStoreUploadSocketType"/>
    <xs:complexType name="VcTrustStoreUploadSocketType">
        <xs:annotation>
            <xs:appinfo>
                <meta:version added-in="5.1" />
                <meta:content-type>application/vnd.vmware.admin.vcTrustStoreUploadSocket</meta:content-type>
            </xs:appinfo>
            <xs:documentation source="since">5.1</xs:documentation>
            <xs:documentation xml:lang="en">
                Upload socket for vCenter trust store.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:FileUploadSocketType" />
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="CatalogSettings" type="CatalogSettingsType" />

    <xs:complexType name="CatalogSettingsType">
        <xs:annotation>
            <xs:appinfo>
                <meta:content-type>application/vnd.vmware.admin.catalogSettings</meta:content-type>
                <meta:version added-in="5.5"/>
            </xs:appinfo>
            <xs:documentation source="since">5.5</xs:documentation>
            <xs:documentation xml:lang="en">
                Catalog settings for this cloud
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="vcloud:ResourceType">
                <xs:sequence>
                    <xs:element name="isSyncEnabled" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                True to enable the synchronization of external catalogs.
                            </xs:documentation>
                            <xs:documentation source="required">true</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SyncStartDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Date and Time at which the synchronization will start.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SyncStopDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Date and Time at which the synchronization will stop.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="RefreshInterval" type="xs:long" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Number of hours between attempts to synchronize with external catalogs. Default is 24 hours.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="OperationLimitsSettings" type="OperationLimitsSettingsType"/>
    <xs:complexType name="OperationLimitsSettingsType">
        <xs:annotation>
            <xs:appinfo>
                <meta:version added-in="20.0"/>
                <meta:content-type>application/vnd.vmware.admin.operationLimitsSettings</meta:content-type>
            </xs:appinfo>
            <xs:documentation source="since">20.0</xs:documentation>
            <xs:documentation xml:lang="en">
            Specifies default operation limits settings for all organizations in
            the system. An operation is defined as resource-intensive if it
            returns a Task object. The default value for all operation limits
            settings is 0, which specifies no limit
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ResourceType">
                <xs:sequence>
                    <xs:element name="RunningPerUser" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="20.0"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Maximum number of resource-intensive operations
                                requested by a user in any organization that
                                can be simultaneously eligible to run when
                                an orgnization specific limit is not set. When a
                                user makes a request that causes this limit
                                to exceed; the requested operation is queued but remains
                                ineligible to run until enough of the user's eligible
                                requests have been run.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="RunningPerOrg" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="20.0"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Maximum number of resource-intensive operations
                                owned by any organization that can be
                                simultaneously eligible to run when an orgnization
                                specific limit is not set. When an organization
                                member makes a request that causes this limit
                                to exceed, the requested operation is queued
                                but remains ineligible to run until enough of the
                                organization's eligible requests have been run.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="QueuedOperationsPerUser" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="20.0"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Maximum number of resource-intensive operations
                                that a user in any organization can queue when
                                an orgnization specific limit is not set.
                                When a user makes a request that causes
                                this limit to exceed, the request fails
                                and returns HTTP status 400.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="QueuedOperationsPerOrg" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="20.0"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Maximum number of queued resource-intensive operations
                                allowed for users in any organization when an
                                orgnization specific limit is not set.
                                When an organization member makes a request
                                that causes this limit to exceed, the
                                request fails and returns HTTP status 400.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                 </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>
