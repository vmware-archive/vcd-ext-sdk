                        <xs:annotation>
                            <xs:documentation source="modifiable">
                                always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                A reference to a source object.
                                One of:&lt;br&gt;
                                vApp&lt;br&gt;
                                vAppTemplate&lt;br&gt;
                                Vm&lt;br&gt;
                                Vms must be powered off and cannot have an
                                independent disk attached.
                            </xs:documentation>
                            <xs:documentation source="required">
                                true
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmGeneralParams"
                        type="VmGeneralParamsType" minOccurs="0"
                        maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo>
                                <meta:version added-in="5.6" />
                            </xs:appinfo>
                            <xs:documentation source="since">
                                5.6
                            </xs:documentation>
                            <xs:documentation source="modifiable">
                                always
                            </xs:documentation>
