                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VAppScopedLocalId"
                        type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">
                                1.0
                            </xs:documentation>
                            <xs:documentation source="modifiable">
                                always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                If Source references a Vm, this value
                                provides a unique identifier for the Vm
                                in the scope of the composed vApp.
                            </xs:documentation>
                            <xs:documentation source="required">
                                false
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="InstantiationParams"
                        type="InstantiationParamsType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="modifiable">
                                always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                If Source references a Vm this can
                                include any of the following OVF
                                sections:&lt;br&gt;
                                VirtualHardwareSection&lt;br&gt;
                                OperatingSystemSection&lt;br&gt;
                                NetworkConnectionSection&lt;br&gt;
                                GuestCustomizationSection.
                            </xs:documentation>
                            <xs:documentation source="required">
                                false
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="NetworkAssignment"
                        type="NetworkAssignmentType" minOccurs="0"
                        maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation source="modifiable">
                                always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                If Source references a Vm, this element
                                maps a network name specified in the Vm
                                to the network name of a vApp network
                                defined in the composed vApp.
                            </xs:documentation>
                            <xs:documentation source="required">
                                false
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="StorageProfile"
                        type="ReferenceType" minOccurs="0"
                        maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo>
                                <meta:version added-in="5.1" />
                            </xs:appinfo>
                            <xs:documentation source="modifiable">
                                always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                If Source references a Vm, this element
                                contains a reference to a storage
                                profile to be used for the Vm. The
                                specified storage profile must exist in
                                the organization vDC that contains the
                                composed vApp. If not specified, the
                                default storage profile for the vDC is
                                used.
                            </xs:documentation>
                            <xs:documentation source="required">
                                false
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="LocalityParams"
                        type="LocalityParamsType" minOccurs="0"
                        maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo>
                                <meta:version added-in="5.6" />
                            </xs:appinfo>
                            <xs:documentation source="since">
                                5.6
                            </xs:documentation>
                            <xs:documentation source="modifiable">
                                always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                Locality parameters provide a hint that may help
                                optimize placement of a VM and an independent a Disk so
                                that the VM can make efficient use of
                                the disk.
                                See &lt;a href="http://kb.vmware.com/kb/2105352"&gt;KB 2105352&lt;/a&gt;.
                            </xs:documentation>
                            <xs:documentation source="required">
                                false
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmCapabilities" type="VmCapabilitiesType" maxOccurs="1" minOccurs="0">
                        <xs:annotation>
                            <xs:appinfo>
                                <meta:version added-in="5.6" />
                            </xs:appinfo>
                            <xs:documentation source="since">
                                5.6
                            </xs:documentation>
                            <xs:documentation source="modifiable">
                                always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                If Source references a Vm, this element
                                describes the capabilities (hot swap, etc.)
                                the instantiated VM should have.
                            </xs:documentation>
                            <xs:documentation source="required">
                                false
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="sourceDelete" type="xs:boolean" use="optional">
                    <xs:annotation>
                        <xs:documentation source="since">1.0</xs:documentation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            True if the source item should be deleted after
                            composition is complete.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

